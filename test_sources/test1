#include "stdio.h"
#include "stdlib.h"
int main()
{
    float t[4][4];
    float z[4][4] = {{1.0,2.0,3.0,4.0},{1.0,2.0,3.0,4.0},{1.0,2.0,3.0,4.0},{1.0,2.0,3.0,4.0}};
    int i;
    int j;
    int a = 2;
    for(i=0;i<=3;i++)
    {
        for (j = 0; j <=3 ; j++) {
            t[i][j] = z[i][j];
        }
    }

    for(i=0;i<=3;i++)
    {
        for (j = 0; j <=3 ; j++) {
            printf("%f ",t[i][j]);
        }
        printf("\n");
    }

    return 0;
}